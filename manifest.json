{
  "name": "HypeBeast Essentials",
  "version": "0.1.3",
  "icons": {
    "48": "static/logo.png",
    "128": "static/logo-128.png"
  },
  "manifest_version": 2,
  "description": "HybeBeast Essentials",
  "options_page": "options.html",
