{
  "name": "HypeBeast Essentials - S",
  "version": "1.0.0",
  "icons": {
    "48": "static/s.png",
    "128": "static/s-128.png"
  },
  "manifest_version": 2,
  "description": "HybeBeast Essentials - S provides assisted checkout for supported websites.",
  "options_page": "options.html",
  "homepage_url": "https://nextwavesolutions.io",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Checkout"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "storage"
  ]
}